record(stringin, "$(P)$(R=)$(S=:)SwVer-I") {
  field(DESC, "Software version")
  field(DTYP, "InstIOStringinObjProp")
  field(INP , "@OBJ=$(OBJ=SCE), PROP=SwVer")
  field(PINI, "YES")
}

record(stringin, "$(P)$(R=)$(S=:)SwBuild-I") {
 field(DESC, "Software build date/timestamp")
